{
    "env": {
        "IDF_PATH": "H:/eps32/esp-idf",
        "toolchain":"H:/eps32/.espressif/tools/xtensa-esp32-elf/esp-2019r2-8.2.0/xtensa-esp32-elf"
	},
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${IDF_PATH}/components/**",
                "${workspaceFolder}/**",
                "${toolchain)/xtensa-esp32-elf/include/**"
            ],
            "browse": {
                "path": [
                    "${IDF_PATH}/components",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": false   
            },
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x86"
        }
    ],
    "version": 4
}